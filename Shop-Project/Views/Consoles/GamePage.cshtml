@model IEnumerable<GroupGameConsole>@{ ViewData["Title"] = "GroupBy";}@foreach (var group in Model){<div class="d-flex justify-content-center container mt-5">    <div class="card p-3 bg-white">        <i class="fa fa-apple"></i>        <div class="about-product text-center mt-2">            <img src="@group.Image" width="300">            <div>                <h4>@group.Name</h4>                <h6 class="mt-0 text-black-50">@group.Price $</h6>            </div>        </div>    </div></div>}