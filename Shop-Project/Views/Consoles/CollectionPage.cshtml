@model Shop_Project.Models.Console@{ ViewData["Title"] = "GroupBy";}<head>    <link rel="stylesheet" href="~/css/mahtel.css" type="text/css" /></head>@foreach (var game in Model.Games){<div class="card mycard" style="width:200px">    <a href="@Url.Action("Details","Games",new { id = game.Id })">        <img class="btn card-img-top" style="height:220px" src="@Html.DisplayFor(modelItem => game.Image)" alt="Card image">    </a>    <div class="card-body">        <h4 class="card-title textsize14">@Html.DisplayFor(modelItem => game.Name)</h4>        <p class="card-text textsize12 text-center"><b>@Html.DisplayFor(modelItem => game.Price)</b></p>        <a class="btn btn-primary detailbtncolor" asp-controller="Games" asp-action="Details" asp-route-id="@game.Id">More Details</a>    </div></div>}@foreach (var consoleVersion in Model.ConsoleVersions){<div class="card mycard" style="width:200px">    <a href="@Url.Action("Details","ConsoleVersions",new { id = consoleVersion.Id })">        <img class="btn card-img-top" style="height:220px" src="@Html.DisplayFor(modelItem => consoleVersion.Image)" alt="Card image">    </a>    <div class="card-body">        <h4 class="card-title textsize14">@Html.DisplayFor(modelItem => consoleVersion.Name)</h4>        <p class="card-text textsize12 text-center"><b>@Html.DisplayFor(modelItem => consoleVersion.Price)</b></p>        <a class="btn btn-primary detailbtncolor" asp-controller="ConsoleVersions" asp-action="Details" asp-route-id="@consoleVersion.Id">More Details</a>    </div></div>}@foreach (var accessory in Model.accessories){<div class="card mycard" style="width:200px">    <a href="@Url.Action("Details","ConsoleVersions",new { id = accessory.Id })">        <img class="btn card-img-top" style="height:220px" src="@Html.DisplayFor(modelItem => accessory.Image)" alt="Card image">    </a>    <div class="card-body">        <h4 class="card-title textsize14">@Html.DisplayFor(modelItem => accessory.Name)</h4>        <p class="card-text textsize12 text-center"><b>@Html.DisplayFor(modelItem => accessory.Price)</b></p>        <a class="btn btn-primary detailbtncolor" asp-controller="Accessories" asp-action="Details" asp-route-id="@accessory.Id">More Details</a>    </div></div>}