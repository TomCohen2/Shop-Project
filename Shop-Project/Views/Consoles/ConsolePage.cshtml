@model IEnumerable<GroupGameConsole>@{ ViewData["Title"] = "GroupBy";}<div id="gameList" class="container mt-3>    <div class="row">        @foreach (var item in Model)        {            <div class="col-3 p-1">                <div class="card">                    <a href="@Url.Action("Details","Games",new { id = item.Id })">                        <img class="btn card-img-top" style="height:220px" src="@Html.DisplayFor(modelItem => item.Image)" alt="Card image">                    </a>                    <div class="card-body">                        <h4 class="card-title textsize14">@Html.DisplayFor(modelItem => item.Name)</h4>                        <br />                        <p class="card-text textsize12 text-center"><b>@Html.DisplayFor(modelItem => item.Price)</b></p>                        <a class="btn btn-primary detailbtncolor" asp-action="Details" asp-route-id="@item.Id">More Details</a>                    </div>                </div>            </div>        }    </div></div>